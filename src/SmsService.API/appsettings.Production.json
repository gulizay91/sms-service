{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error",
      "ProxyLogger": "Error",
      "ConsoleLogger": "Warning"
    }
  },
  "Services": {
    "TwilioService": {
      "AccountSID": "${TWILIO_ACCOUNT_SID}",
      "AuthToken": "${TWILIO_AUTH_TOKEN}",
      "PhoneNumber": "+11111111111",
      "SmsProviderSettings": {
        "Enable": false,
        "ActiveCountryCodes": [ "+90" ]
      }
    },
    "IletiMerkeziService": {
      "Key": "${ILETIMERKEZI_KEY}",
      "Hash": "${ILETIMERKEZI_HASH}",
      "Sender": "${SENDER_NAME}>",
      "ProxySettings": {
        "Url": "https://api.iletimerkezi.com",
        "Timeout": 3,
        "RetryAttempts": 2
      },
      "SmsProviderSettings": {
        "Enable": true,
        "ActiveCountryCodes": [ "+90" ]
      }
    }
  }
}